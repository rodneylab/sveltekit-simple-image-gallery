{
	"name": "sveltekit-simple-image-gallery",
	"version": "0.0.1",
	"license": "BSD-3-Clause",
	"description": "Simple Svelte responsive image gallery: create a ribbon gallery, using Svelte dimension bindings to maintain the aspect ratio of all images.",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"check": "svelte-check --fail-on-warnings",
		"lint:css": "stylelint \"src/**/*.{css,svelte}\"",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.3.0",
		"@fontsource/inter": "^4.5.14",
		"@iconify/svelte": "^2.2.1",
		"@rodneylab/sveltekit-components": "^2.0.0",
		"@sveltejs/adapter-static": "1.0.0",
		"@sveltejs/kit": "1.0.0",
		"commitlint": "^17.3.0",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.2",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"sharp": "~0.31.2",
		"stylelint": "^14.16.0",
		"stylelint-config-recommended": "^9.0.0",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.2",
		"vanilla-lazyload": "^17.8.3",
		"vite": "^4.0.1",
		"vite-imagetools": "^4.0.11"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/sveltekit-simple-image-gallery"
