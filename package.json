		"@sveltejs/adapter-static": "1.0.0-next.43",
		"@sveltejs/kit": "next",
		"commitlint": "^17.0.3",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"leaflet": "^1.8.0",
		"postcss-html": "^1.5.0",
		"prettier": "^2.7.1",
